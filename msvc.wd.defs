# MSVC definitions for Windows-dependent Makefile.wd.
O = .obj
RCO = .res
ALLDEPS = $**
CXX = cl
LINK = link
CFLAGs = -nologo
LDFLAGS = -nologo -subsystem:windows
LDO = -out:
GUILIBS = kernel32.lib user32.lib gdi32.lib
LIBS = $(GUILIBS) opengl32.lib glu32.lib winmm.lib
WINDRES = rc -r
RESOUT =
RMF = del
